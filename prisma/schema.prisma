generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Review {
  id      Int    @id @unique
  rating  Int    @default(0) @db.SmallInt
  Message String

  @@map(name: "reviews")
}
